#include "stm32f0xx.h"
void inita (void);

int main(void)
{
	inita;
	for(;;);
}

// Function to initialize GPIOA ports (0 - 13)
// for outputs to LED Dot Matrix
void inita (void)
{
	// Start the RCC clock
	RCC -> AHBENR |= RCC_AHBENR_GPIOBEN;

	GPIOA -> MODER &= 0xf0000000;
	GPIOA -> MODER |= 0x05555555;
}
